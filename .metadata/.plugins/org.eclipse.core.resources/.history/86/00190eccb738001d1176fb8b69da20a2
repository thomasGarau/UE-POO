
public class ManipRob {
	
	 public static void main (String[] args){
		 Robot robot1 = new Robot("toto", 10, 20, 3);
		 Robot robot2 = new Robot("titi");
		 
		 robot1.moove();
		 robot1.moove();
		 robot2.moove();
		 robot2.changeOrientation(2);
		 robot2.moove();
		 robot2.moove();
		 
		 robot1.afficheToi();
		 robot2.afficheToi();
		 System.Out("aa");
	 }
}
