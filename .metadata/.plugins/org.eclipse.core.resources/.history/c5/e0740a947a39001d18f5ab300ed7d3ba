
public class test {

	public static void main(String[] args) {
		Entier entier = new Entier(0, 10, 5);
		Entier entierFou = new Entier(0,10,5);
		for(int i =0; i < 20; i++) {
			entier.Incremente();
			entierFou.Incremente();
		}
		System.out.print("entier " + entier.getValeur() + " entierFou " + entierFou.getValeur()); 
	}

}
